<h1>Add A New Squad</h1>

<form  action="/squads" method="post">
  <div class="form-group">
    <label for="squadName">Squad Name</label>
    <input type="text" name="squadName" class="form-control" required>
  </div>
  <div class="form-group">
    <label for="squadGoal">Squad Goal</label>
    <input type="text" name="squadGoal" class="form-control" required>
  </div>
  <button type="submit" name="button" class="btn btn-warning">Assemble Squad</button>
</form>
